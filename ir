declare void @scheduleEventData(ptr, ptr)

declare void @exitEvent(ptr)

define i32 @mainLLVM() {
entry:
  %duracionVideo_ptr = alloca float, align 4
  %myName_ptr = alloca ptr, align 8
  %TFG_ptr = alloca i1, align 1
  %flotante_ptr = alloca float, align 4
  %entero_ptr = alloca i32, align 4
  store i32 -10, ptr %entero_ptr, align 4
  store float 0x40091EB860000000, ptr %flotante_ptr, align 4
  store i1 true, ptr %TFG_ptr, align 1
